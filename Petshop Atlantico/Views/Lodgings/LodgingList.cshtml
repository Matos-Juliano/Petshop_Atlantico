@model IEnumerable<Petshop_Atlantico.Models.Lodging>

@{
    ViewData["Title"] = "LodgingList";
}

<h1>LodgingList</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OccupationStatus)
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OccupationStatus)
            </td>           
        </tr>
}
    </tbody>
</table>
